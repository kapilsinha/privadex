{
  "name": "kusama-balances",
  "private": true,
  "scripts": {
    "build": "rm -rf lib && tsc",
    "init": "node db/init.js",
    "db:migrate": "npx squid-typeorm-migration apply",
    "update": "npx npm-check-updates --filter /subsquid/ --upgrade && npm i -f",
    "schema:reset": "",
    "processor:migrate": "echo skip",
    "processor:start": "node lib/processor.js",
    "query-node:start": "squid-graphql-server",
    "lint": "npm run lint:prod -- --fix",
    "lint:prod": "eslint . --ext .ts --ignore-path .gitignore --max-warnings 0"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.6.3",
    "@ethersproject/address": "^5.6.1",
    "@subsquid/archive-registry": "1.0.17",
    "@subsquid/big-decimal": "^0.0.0",
    "@subsquid/cli": "0.7.4",
    "@subsquid/frontier": "0.1.0",
    "@subsquid/graphql-server": "3.2.4",
    "@subsquid/ss58": "0.1.2",
    "@subsquid/substrate-processor": "2.1.0",
    "@subsquid/typeorm-migration": "0.1.4",
    "@subsquid/typeorm-store": "0.1.5",
    "dotenv": "^10.0.0",
    "ethers": "^5.6.9",
    "graphql-fields": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "snake-case": "^3.0.4",
    "type-graphql": "^1.2.0-rc.1",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@subsquid/substrate-metadata-explorer": "1.0.9",
    "@subsquid/substrate-typegen": "2.0.0",
    "@subsquid/typeorm-codegen": "0.3.1",
    "@types/big.js": "^6.1.5",
    "@types/graphql-fields": "^1.3.4",
    "@types/node": "^16.11.17",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "prettier": "2.3.0",
    "prettier-eslint": "^13.0.0",
    "typescript": "~4.5.4"
  }
}
